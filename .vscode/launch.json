{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/frontend",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack://_N_E/./*": "${workspaceFolder}/frontend/src/*",
        "webpack://_N_E/./src/*": "${workspaceFolder}/frontend/src/*",
        "webpack:///./*": "${workspaceFolder}/frontend/src/*",
        "webpack:///./src/*": "${workspaceFolder}/frontend/src/*"
      },
      "skipFiles": [
        "${workspaceFolder}/frontend/node_modules/**",
        "${workspaceFolder}/frontend/.next/**",
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach to Chrome",
      "request": "attach",
      "type": "chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}/frontend",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack://_N_E/./*": "${workspaceFolder}/frontend/src/*",
        "webpack://_N_E/./src/*": "${workspaceFolder}/frontend/src/*",
        "webpack:///./*": "${workspaceFolder}/frontend/src/*",
        "webpack:///./src/*": "${workspaceFolder}/frontend/src/*"
      },
      "skipFiles": [
        "${workspaceFolder}/frontend/node_modules/**",
        "${workspaceFolder}/frontend/.next/**",
        "<node_internals>/**"
      ]
    }
  ]
}
